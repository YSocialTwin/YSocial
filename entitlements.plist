<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Allow loading of unsigned libraries and frameworks -->
    <!-- This is necessary for PyInstaller single-file executables -->
    <!-- that extract and load dynamic libraries at runtime -->
    <key>com.apple.security.cs.allow-unsigned-executable-memory</key>
    <true/>
    
    <!-- Allow loading of dynamically linked libraries -->
    <key>com.apple.security.cs.allow-dyld-environment-variables</key>
    <true/>
    
    <!-- Disable library validation for ad-hoc signed apps -->
    <!-- This allows the app to load its own extracted libraries -->
    <key>com.apple.security.cs.disable-library-validation</key>
    <true/>
    
    <!-- Allow JITT (Just-In-Time compilation) for Python runtime -->
    <key>com.apple.security.cs.allow-jit</key>
    <true/>
</dict>
</plist>
